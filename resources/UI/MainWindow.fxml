<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="707.0" prefWidth="1091.0" stylesheets="@style/MainWindow.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.MainWindow">
   <children>
      <MenuBar fx:id="menubar" layoutY="2.0" prefHeight="25.0" prefWidth="1091.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <MenuBar AnchorPane.rightAnchor="0.0">
         <menus>
            <Menu fx:id="profileButton" mnemonicParsing="false" text="Profile">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
         </menus>
      </MenuBar>
      <VBox layoutY="27.0" prefHeight="684.0" prefWidth="242.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="25.0">
         <children>
            <TitledPane animated="false" collapsible="false" prefHeight="400.0" prefWidth="254.0" text="Teams">
               <content>
                  <ScrollPane prefHeight="487.0" prefWidth="240.0">
                     <content>
                        <VBox fx:id="teams" prefHeight="373.0" prefWidth="240.0" />
                     </content>
                  </ScrollPane>
               </content>
               <font>
                  <Font name="Trebuchet MS" size="12.0" />
               </font>
            </TitledPane>
            <TitledPane animated="false" prefHeight="281.0" prefWidth="242.0" text="Settings">
               <content>
                  <ScrollPane prefHeight="307.0" prefWidth="285.0">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="283.0" prefWidth="240.0">
                           <children>
                              <Button fx:id="addTeamButton" layoutX="45.0" layoutY="14.0" mnemonicParsing="false" onAction="#createTeamDialog" prefHeight="25.0" prefWidth="150.0" text="Add Team">
                                 <font>
                                    <Font name="Trebuchet MS" size="12.0" />
                                 </font>
                              </Button>
                              <Button fx:id="inviteButton" disable="true" layoutX="44.0" layoutY="53.0" mnemonicParsing="false" onAction="#createTeamDialog" text="Select a Team to continue" textAlignment="CENTER">
                                 <font>
                                    <Font name="Trebuchet MS" size="12.0" />
                                 </font>
                              </Button>
                              <Button fx:id="addTask" disable="true" layoutX="43.0" layoutY="93.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="154.0" text="Add Task" textAlignment="CENTER">
                                 <font>
                                    <Font name="Trebuchet MS" size="12.0" />
                                 </font>
                              </Button>
                           </children>
                        </AnchorPane>
                    </content>
                  </ScrollPane>
               </content>
               <font>
                  <Font name="Trebuchet MS" size="12.0" />
               </font>
            </TitledPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
